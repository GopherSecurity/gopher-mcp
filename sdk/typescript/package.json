{
  "name": "@mcp/filter-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for MCP Filter and Authentication C API",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/src/index.js",
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./auth": {
      "require": "./dist/src/auth.js",
      "import": "./dist/src/auth.js",
      "types": "./dist/src/auth.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest --no-cache --no-coverage",
    "test:auth": "jest --no-cache --no-coverage auth",
    "test:watch": "jest --watch --no-cache",
    "test:debug": "jest --no-cache --no-coverage --verbose --runInBand",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "server:hybrid": "tsx ../../examples/typescript/calculator-hybrid/calculator-server-hybrid.ts",
    "example:async": "tsx examples/async-transport-example.ts",
    "example:assembler": "tsx examples/assembler-example.ts",
    "example:basic": "tsx examples/basic-usage.ts",
    "example:filtered": "tsx examples/filtered-transport-example.ts"
  },
  "keywords": [
    "mcp",
    "filter",
    "authentication",
    "jwt",
    "oauth",
    "jwks",
    "network",
    "proxy",
    "gateway",
    "typescript",
    "ffi",
    "c-api"
  ],
  "author": "MCP Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/modelcontextprovider/gopher-mcp.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "jose": "^5.10.0",
    "koffi": "^2.13.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.123",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0 <19.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
