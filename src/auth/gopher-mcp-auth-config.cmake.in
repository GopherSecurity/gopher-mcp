@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find required dependencies
find_dependency(OpenSSL REQUIRED)
find_dependency(CURL REQUIRED)

# Import targets
include("${CMAKE_CURRENT_LIST_DIR}/gopher-mcp-auth-targets.cmake")

# Set variables for compatibility
set(GOPHER_MCP_AUTH_FOUND TRUE)
set(GOPHER_MCP_AUTH_LIBRARIES gopher-mcp-auth::gopher-mcp-auth)
set(GOPHER_MCP_AUTH_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/include")

check_required_components(gopher-mcp-auth)